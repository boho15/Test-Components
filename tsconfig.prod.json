{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "es2020",
    "declaration": true,
    "declarationMap": false,
    "sourceMap": false,
    "emitDecoratorMetadata": false,
    "importHelpers": true,
    "skipLibCheck": true,
    "strict": true,
    "removeComments": true
  },
  "angularCompilerOptions": {
    "enableIvy": true,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true
  }
}
